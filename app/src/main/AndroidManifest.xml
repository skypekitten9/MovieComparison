<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ass3">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/movieicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/movieicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ass3"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity" android:configChanges="orientation|keyboardHidden|screenSize">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PopUpWindow"
            android:theme= "@style/Theme.Transparent"

            />
    </application>

</manifest>